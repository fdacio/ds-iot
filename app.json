{
  "app": {
    "name" : "DS-IoT",
    "version" : "1.0.9"
  },
  "expo": {
    "name": "DS-IOT",
    "slug": "ds-iot",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "br.com.daciosoftware.dsiot",
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "buildNumber": "1.0.9",
      "bundleIdentifier": "br.com.daciosoftware.dsiot"
    },
    "extra": {
      "eas": {
        "projectId": "d5191b00-386c-40dc-9810-350c3d23d6f1"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "networkInspector": true
          }
        }
      ]
    ]
  }
}
